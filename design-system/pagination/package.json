		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "*",
		"@af/integration-testing": "*",
		"@af/visual-regression": "*",
		"@atlaskit/code": "^16.0.0",
		"@atlaskit/docs": "*",
		"@atlaskit/heading": "^5.1.0",
		"@atlaskit/link": "*",
