{
	"name": "@atlaskit/editor-plugin-find-replace",
	"version": "7.0.5",
	"description": "find replace plugin for @atlaskit/editor-core",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
		"team": "Editor: Lego"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"*.compiled.css"
	],
	"atlaskit:src": "src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./styles": "./src/ui/styles.ts"
	},
	"dependencies": {
		"@atlaskit/button": "workspace:^",
		"@atlaskit/editor-plugin-analytics": "workspace:^",
		"@atlaskit/editor-plugin-card": "workspace:^",
		"@atlaskit/editor-plugin-expand": "workspace:^",
		"@atlaskit/editor-plugin-mentions": "workspace:^",
		"@atlaskit/editor-plugin-primary-toolbar": "workspace:^",
		"@atlaskit/editor-prosemirror": "workspace:^",
		"@atlaskit/editor-shared-styles": "workspace:^",
		"@atlaskit/form": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/icon-lab": "workspace:^",
		"@atlaskit/mention": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/textfield": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tmp-editor-statsig": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@atlaskit/tooltip": "workspace:^",
		"@babel/runtime": "root:*",
		"@emotion/react": "root:*",
		"classnames": "root:*",
		"lodash": "root:*",
		"raf-schd": "root:*"
	},
	"devDependencies": {
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "root:*",
		"mockdate": "root:*",
		"raf-stub": "root:*",
		"react-dom": "root:*"
	},
	"peerDependencies": {
		"@atlaskit/editor-common": "workspace:^",
		"react": "^18.2.0",
		"react-intl-next": "*"
	},
	"techstack": {
		"@atlassian/frontend": {
			"code-structure": [
				"editor-plugin"
			],
			"import-structure": [
				"atlassian-conventions"
			],
			"circular-dependencies": [
				"file-and-folder-level"
			]
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"analytics": [
				"analytics-next"
			],
			"design-tokens": [
				"color"
			],
			"theming": [
				"react-context"
			],
			"ui-components": [
				"lite-mode"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"emotion",
				"emotion"
			],
			"imports": [
				"import-no-extraneous-disable-for-examples-and-docs"
			]
		}
	},
	"stricter": {
		"no-unused-dependencies": {
			"exclude": [
				"@atlaskit/editor-plugin-analytics"
			]
		}
	},
	"platform-feature-flags": {
		"platform_editor_toolbar_responsive_fixes": {
			"type": "boolean"
		},
		"platform_editor_dec_a11y_fixes": {
			"type": "boolean"
		}
	}
}
