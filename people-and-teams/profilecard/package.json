		"rovo_dev_themed_identity_card": {
			"type": "boolean"
		},
		"enable_profilecard_text_truncation_tooltip": {
			"type": "boolean"
		},
		"people-teams-fix-no-literal-string-in-jsx": {
			"type": "boolean"
		},
		"people-teams-deprecate-profilecard-teams": {
			"type": "boolean"
		},
		"rovo_display_ai_disclaimer_on_agent_profile_card": {
			"type": "boolean"
		},
		"rovo_agent_show_creator_on_profile_card_fix": {
			"type": "boolean"
		}
	},
	"sideEffects": [
		"*.compiled.css"
	]
}
