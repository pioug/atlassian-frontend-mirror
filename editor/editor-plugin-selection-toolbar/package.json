			"ui-components": [
				"lite-mode"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"compiled"
			]
		}
	},
	"platform-feature-flags": {
		"platform_editor_user_preferences_provider_update": {
			"type": "boolean"
		},
		"platform_editor_use_preferences_plugin": {
			"type": "boolean"
		}
