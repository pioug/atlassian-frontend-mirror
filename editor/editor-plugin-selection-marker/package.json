		"@emotion/react": "root:*",
		"lodash": "root:*"
	},
	"peerDependencies": {
		"@atlaskit/editor-common": "workspace:^",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@testing-library/react": "root:*",
		"wait-for-expect": "root:*"
	},
	"techstack": {
