{
	"extends": "../../../tsconfig.node.json",
	"include": ["./src/**/*.ts", "./src/**/*.tsx"],
	"exclude": [
		"**/docs/**/*",
		"**/__tests__/**/*",
		"**/vr-tests/**/*",
