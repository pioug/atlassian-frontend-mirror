{
	"name": "@atlaskit/townsquare-emoji-provider",
	"version": "1.0.4",
	"description": "Townsquare emoji provider",
	"atlassian": {
		"team": "Townsquare"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-monorepo",
	"main": "dist/cjs/provider.js",
	"module": "dist/esm/provider.js",
	"module:es2019": "dist/es2019/provider.js",
	"types": "dist/types/provider.d.ts",
	"typesVersions": {
		">=4.5 <5.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": false,
	"atlaskit:src": "src/provider.ts",
	"exports": {
		".": "./src/provider.ts",
		"./project-icon": "./src/ProjectIcon.tsx"
	},
	"dependencies": {
		"@atlaskit/avatar": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/emoji": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/visual-regression": "workspace:^",
		"@testing-library/react": "^16.3.0"
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": [
				"atlassian-conventions"
			],
			"circular-dependencies": [
				"file-and-folder-level"
			]
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"analytics": [
				"analytics-next"
			],
			"design-tokens": [
				"color"
			],
			"theming": [
				"react-context"
			],
			"ui-components": [
				"lite-mode"
			],
			"deprecation": [
				"no-deprecated-imports"
			],
			"styling": [
				"static",
				"compiled"
			],
			"imports": [
				"import-no-extraneous-disable-for-examples-and-docs"
			]
		}
	},
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	}
}
