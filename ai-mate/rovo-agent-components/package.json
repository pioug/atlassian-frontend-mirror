{
	"name": "@atlaskit/rovo-agent-components",
	"version": "3.23.0",
	"description": "This package host public components related to rovo agents, the components here are needed for other public atlaskit packages",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
		"team": "Rovo Agents",
		"website": {
			"name": "RovoAgentComponents",
			"category": "Components"
		}
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-monorepo",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <5.4": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"*.compiled.css"
	],
	"atlaskit:src": "src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./agent-verified": "./src/ui/agent-verified/index.tsx",
		"./agent-verification-dropdown-item": "./src/ui/agent-verification-dropdown-item/index.tsx"
	},
	"dependencies": {
		"@atlaskit/afm-i18n-platform-ai-mate-rovo-agent-components": "root:*",
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/avatar": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/dropdown-menu": "workspace:^",
		"@atlaskit/flag": "workspace:^",
		"@atlaskit/heading": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/logo": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/rovo-agent-analytics": "workspace:^",
		"@atlaskit/skeleton": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@atlaskit/tooltip": "workspace:^",
		"@babel/runtime": "root:*",
		"@compiled/react": "root:*",
		"react-dom": "root:*",
		"react-intl-next": "root:*",
		"react-magnetic-di": "root:*",
		"react-relay": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/ssr": "workspace:^",
		"@atlassian/agent-studio-test-utils": "workspace:^",
		"@atlassian/feature-flags-test-utils": "workspace:^",
		"@atlassian/testing-library": "workspace:^",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "root:*",
		"@types/relay-test-utils": "root:*",
		"relay-test-utils": "^14.1.0",
		"wait-for-expect": "root:*"
	},
	"techstack": {
		"@atlassian/frontend": {
			"code-structure": [
				"tangerine-next"
			],
			"import-structure": [
				"atlassian-conventions"
			],
			"circular-dependencies": [
				"file-and-folder-level"
			]
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"analytics": [
				"analytics-next"
			],
			"design-tokens": [
				"color"
			],
			"theming": [
				"react-context"
			],
			"ui-components": [
				"lite-mode"
			],
			"deprecation": [
				"no-deprecated-imports"
			],
			"styling": [
				"static",
				"compiled"
			],
			"imports": [
				"import-no-extraneous-disable-for-examples-and-docs"
			]
		}
	},
	"techstackIgnore": {
		"stricter": [
			"@atlassian/tangerine/project-structure"
		]
	},
	"platform-feature-flags": {
		"rovo_chat_bugfix_agent_avatar_squish": {
			"type": "boolean"
		},
		"dst-a11y__replace-anchor-with-link__ai-mate": {
			"type": "boolean"
		},
		"should-render-to-parent-should-be-true-ai-mate": {
			"type": "boolean"
		},
		"jira_ai_force_rovo_dev_avatar": {
			"type": "boolean"
		},
		"rovo_agent_empty_state_refresh": {
			"type": "boolean"
		},
		"rovo_dev_themed_identity_card": {
			"type": "boolean"
		},
		"rovo_agents_agent_verification": {
			"type": "boolean"
		}
	}
}
