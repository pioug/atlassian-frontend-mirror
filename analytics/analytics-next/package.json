		"./createAndFireEvents": "./src/utils/createAndFireEvent.ts",
		".": "./src/index.ts"
	},
	"dependencies": {
		"@atlaskit/analytics-next-stable-react-context": "1.0.1",
		"@atlaskit/platform-feature-flags": "^0.3.0",
		"@babel/runtime": "^7.0.0",
		"prop-types": "^15.5.10",
		"use-memo-one": "^1.1.1"
	},
	"peerDependencies": {
