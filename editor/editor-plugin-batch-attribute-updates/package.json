		"releaseModel": "continuous",
		"website": {
			"name": "EditorPluginBatchAttributeUpdate",
			"category": "Components"
		}
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-monorepo",
	"main": "dist/cjs/index.js",
