{
	"name": "@atlaskit/tag",
	"version": "14.2.4",
	"description": "A tag labels UI objects for quick recognition and navigation.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"homepage": "https://atlassian.design/components/tag/",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Tag",
			"category": "Status indicators",
			"subPages": [
				{
					"title": "Tag",
					"id": "tag",
					"status": {
						"type": "alpha",
						"description": "This is an experiment under a feature flag. We may make frequent breaking changes with limited notice. We don't recommend using this without support from our team."
					}
				},
				{
					"title": "AvatarTag",
					"id": "avatar-tag",
					"status": {
						"type": "alpha"
					}
				}
			]
		}
	},
	"exports": {
		"./simple-tag": "./src/tag/simple-tag.tsx",
		"./removable-tag": "./src/tag/removable-tag.tsx",
		"./avatar-tag": "./src/tag-new/avatar-tag.tsx",
		".": "./src/index.tsx"
	},
	"dependencies": {
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/avatar": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/motion": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*",
		"@compiled/react": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/section-message": "workspace:^",
		"@atlaskit/tag-group": "workspace:^",
		"@atlassian/feature-flags-test-utils": "workspace:^",
		"@atlassian/ssr-tests": "workspace:^",
		"@testing-library/react": "^16.3.0",
		"react-dom": "root:*"
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"emotion",
				"static"
			]
		}
	},
	"platform-feature-flags": {
		"platform-visual-refresh-icons": {
			"type": "boolean"
		},
		"platform-dst-lozenge-tag-badge-visual-uplifts": {
			"type": "boolean"
		}
	}
}
