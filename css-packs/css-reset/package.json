  "dependencies": {
    "@atlaskit/theme": "^11.0.0",
    "fbjs": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@atlaskit/docs": "*",
    "mkdirp": "^0.5.1",
    "ts-node": "^6.1.1",
    "typescript": "3.9.6"
  },
  "keywords": [
    "atlaskit",
    "css",
    "ui"
  ]
