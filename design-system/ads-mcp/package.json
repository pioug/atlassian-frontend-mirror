{
	"name": "@atlaskit/ads-mcp",
	"version": "0.13.7",
	"description": "The official Atlassian Design System MCP server to develop apps and user interfaces matching the Atlassian style.",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"homepage": "https://atlassian.design/llms.txt",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
		"team": "Design System Team"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"scripts": {
		"ak-postbuild": "yarn workspace @atlassian/structured-docs build",
		"prepare": "yarn ak-postbuild"
	},
	"bin": "index.js",
	"main": "dist/cjs/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <5.4": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"atlaskit:src": "src/index.tsx",
	"exports": {
		".": {
			"types": "./src/index.tsx",
			"publish": "./src/index.tsx",
			"default": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"platform-feature-flags": {
		"design_system_mcp_structured_content": {
			"type": "boolean"
		}
	},
	"dependencies": {
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@axe-core/playwright": "root:*",
		"@axe-core/puppeteer": "root:*",
		"@babel/runtime": "root:*",
		"@modelcontextprotocol/sdk": "root:*",
		"axe-core": "root:*",
		"esbuild-register": "root:*",
		"fuse.js": "root:*",
		"jsdom": "root:*",
		"playwright": "root:*",
		"puppeteer": "root:*",
		"zod": "root:*",
		"zod-to-json-schema": "root:*"
	},
	"optionalDependencies": {
		"@atlassiansox/analytics-node-client": "root:*"
	},
	"devDependencies": {
		"ts-node": "root:*"
	}
}
