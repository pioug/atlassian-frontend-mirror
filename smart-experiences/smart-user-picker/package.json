{
	"name": "@atlaskit/smart-user-picker",
	"version": "8.9.3",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"description": "Pick users from a ranked user list based on provided context",
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": false,
	"atlaskit:src": "src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./i18n/*": {
			"publish": null,
			"default": "./src/i18n/*.ts"
		},
		"./option": "./src/option.ts"
	},
	"atlassian": {
		"team": "Search Platform: Search Experience",
		"slack": {
			"channel": "#search-plex-prs",
			"notifications": {
				"mergedPR": {
					"message": "A PR was merged in @atlaskit/smart-user-picker without maintainer approval."
				}
			}
		}
	},
	"dependencies": {
		"@atlaskit/afm-i18n-platform-smart-experiences-smart-user-picker": "root:*",
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/ufo": "workspace:^",
		"@atlaskit/user-picker": "workspace:^",
		"@babel/runtime": "root:*",
		"lodash": "root:*",
		"memoize-one": "root:*",
		"react-intl-next": "root:*",
		"uuid": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@atlaskit/select": "workspace:^",
		"@atlaskit/util-data-test": "workspace:^",
		"@atlassian/a11y-jest-testing": "workspace:^",
		"@testing-library/dom": "root:*",
		"@testing-library/react": "root:*",
		"@testing-library/user-event": "root:*",
		"enzyme": "root:*",
		"fetch-mock": "root:*",
		"graphql-tag": "root:*",
		"mock-apollo-client": "root:*"
	},
	"platform-feature-flags": {
		"twcg-444-invite-usd-improvements-m2-gate": {
			"type": "boolean"
		},
		"smart-user-picker-load-options-gate": {
			"type": "boolean"
		},
		"smart-user-picker-managed-teams-gate": {
			"type": "boolean"
		},
		"smart-user-picker-restrict-to-gate": {
			"type": "boolean"
		}
	}
}
