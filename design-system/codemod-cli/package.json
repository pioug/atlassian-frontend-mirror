{
	"name": "@atlaskit/codemod-cli",
	"version": "0.34.1",
	"description": "A cli for distributing codemods for atlassian-frontend components and services",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"atlaskit:src": "src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"publish": "./src/index.ts",
			"default": "./index.js"
		},
		"./cli": "./src/cli.ts",
		"./filepath": "./src/filepath.ts",
		"./main": "./src/main.ts",
		"./sinceRef": "./src/sinceRef.ts",
		"./transforms": "./src/transforms.ts",
		"./types": "./src/types.ts",
		"./utils": "./src/utils.ts"
	},
	"atlassian": {
		"team": "Design System Team"
	},
	"scripts": {
		"prestart": "cd ../../ && yarn build @atlaskit/codemod-utils tokens",
		"start": "./bin/codemod-cli.js"
	},
	"bin": "./bin/codemod-cli.js",
	"dependencies": {
		"@atlaskit/codemod-utils": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/icon-lab": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*",
		"@codeshift/utils": "root:*",
		"@hypermod/utils": "root:*",
		"@manypkg/find-root": "root:*",
		"@types/jscodeshift": "root:*",
		"chalk": "root:*",
		"color-diff": "root:*",
		"enquirer": "root:*",
		"glob": "root:*",
		"jscodeshift": "root:*",
		"meow": "root:*",
		"projector-spawn": "root:*",
		"read-pkg-up": "root:*",
		"semver": "root:*",
		"simple-git": "root:*",
		"uuid": "root:*"
	},
	"devDependencies": {
		"@af/formatting": "workspace:^",
		"@types/color-diff": "root:*",
		"prettier": "root:*",
		"ts-node": "root:*"
	},
	"homepage": "https://atlaskit.atlassian.com/packages/monorepo-tooling/codemod-cli"
}
