		"@repo/internal": {
			"ui-components": [
				"lite-mode"
			],
			"analytics": [
				"analytics-next"
			],
			"design-tokens": [
				"color"
			],
			"theming": [
				"react-context"
			],
			"deprecation": [
				"no-deprecated-imports"
			]
		}
	},
	"platform-feature-flags": {
		"analytics-next-use-modern-context_jira": {
			"type": "boolean"
		}
	}
